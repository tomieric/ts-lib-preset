{
  "name": "ts-lib-preset",
  "version": "0.0.1",
  "description": "The minimal TypeScript library starter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "tommieric <tomieric@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "coverage": "jest --ci --silent --coverage",
    "lint": "eslint --fix --ext js,jsx,.ts,.tsx",
    "test": "jest",
    "commit": "git-cz -e"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^3.3.0",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "commitizen": "^4.2.4",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "git-cz": "^4.8.0",
    "jest": "^27.3.1",
    "pinia": "^2.0.4",
    "prettier": "^2.4.1",
    "release-it": "^14.11.8",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tsup": "^5.7.4",
    "typescript": "^4.5.2",
    "vue": "^3.2.22"
  },
  "peerDependencies": {
    "pinia": "^2.0.4"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
